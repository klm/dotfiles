alias h='history'
alias c='clear'
alias rbash='source ~/.bash_profile' #reload it

alias sup='svn up'
alias sst='svn st'
alias sc='script/console'
alias ss="script/server"

#(i)gnore grep -- ignores .svn directories/files
alias igrep="find . -path '*/.svn' -prune -o -type f -print0 | xargs -0 grep -I -n -e"

